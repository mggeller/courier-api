{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "dist/api/index.js"
    },
    {
      "src": "/delivery-orders",
      "dest": "dist/api/index.js"
    },
    {
      "src": "/orders",
      "dest": "dist/api/index.js"
    },
    {
      "src": "/orders/(.*)",
      "dest": "dist/api/index.js"
    }
  ]
}
